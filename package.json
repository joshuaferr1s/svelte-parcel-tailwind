    "eslint-plugin-svelte3": "^2.7.3",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-svelte": "^4.0.6",
    "svelte": "^3.24.0",
    "tailwindcss-font-inter": "^1.0.8"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "autoprefixer": "^9.8.5",
    "tailwindcss": "^1.5.1"
  }
